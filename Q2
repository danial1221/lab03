using System;

class Program
{
    static int Square(int num)
    {
        return num * num;
    }

    static void Main()
    {
        Console.WriteLine("Squares of the first 10 num:\n");

        for (int i=1;i<=10;i++)
        {
            int square = Square(i);
            Console.WriteLine($"The square of {i} is {square}");
        }
    }
}
